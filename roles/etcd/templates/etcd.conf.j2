ETCD_NAME="{{ etcd_hostname_name }}"
ETCD_LISTEN_CLIENT_URLS="http://{{ etcd_hostname_ip }}:{{ etcd_ports['client'] }},http://127.0.0.1:{{ etcd_ports['client'] }}"
ETCD_ADVERTISE_CLIENT_URLS="http://{{ etcd_hostname_ip }}:{{ etcd_ports['client'] }}"
ETCD_LISTEN_PEER_URLS="http://{{ etcd_hostname_ip }}:{{ etcd_ports['peer'] }}"
ETCD_INITIAL_ADVERTISE_PEER_URLS="http://{{ etcd_hostname_ip }}:{{ etcd_ports['peer'] }}"
ETCD_INITIAL_CLUSTER_TOKEN="{{ etcd_cluster_token }}"
ETCD_INITIAL_CLUSTER="{% for init_cluster in etcd_init_cluster %}{{ init_cluster.host }}=http://{{ init_cluster.ip }}:{{ etcd_ports['peer'] }}{% if not loop.last %},{% endif %}{% endfor %}"
ETCD_INITIAL_CLUSTER_STATE="{{ etcd_init_cluster_state }}"
ETCD_DATA_DIR="{{ etcd_data_dir }}"
ETCD_ENABLE_V2="{{ etcd_enable_v2 }}"
ETCD_ELECTION_TIMEOUT="5000"
ETCD_HEARTBEAT_INTERVAL="1000"
ETCD_INITIAL_ELECTION_TICK_ADVANCE="false"
